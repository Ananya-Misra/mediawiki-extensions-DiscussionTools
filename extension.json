{
	"name": "DiscussionTools",
	"license-name": "MIT",
	"version": "0.0.0",
	"namemsg": "discussiontools",
	"descriptionmsg": "discussiontools-desc",
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.34.0",
		"extensions": {
			"VisualEditor": ">= 0.1.1"
		}
	},
	"MessagesDirs": {
		"DiscussionTools": [ "i18n" ]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "DiscussionTools/modules"
	},
	"ResourceModules": {
		"ext.discussionTools.parser": {
			"packageFiles": [
				"parser.js",
				"lib/moment-timezone/moment-timezone-with-data-1970-2030.js",
				{
					"name": "data.json",
					"callback": "DiscussionToolsHooks::getLocalData"
				}
			],
			"dependencies": [
				"moment",
				"mediawiki.util",
				"mediawiki.Uri",
				"mediawiki.Title"
			]
		},
		"ext.discussionTools.init": {
			"packageFiles": [
				"dt.init.js"
			],
			"dependencies": [
				"mediawiki.Uri",
				"ext.discussionTools.parser"
			]
		},
		"ext.discussionTools.debug": {
			"packageFiles": [
				"dt.debug.js",
				"highlighter.js"
			],
			"styles": "highlighter.css",
			"dependencies": [
				"ext.discussionTools.parser"
			]
		}
	},
	"QUnitTestModule": {
		"localBasePath": "tests/qunit",
		"remoteExtPath": "DiscussionTools/tests/qunit",
		"packageFiles": [
			"index.js",
			"utils.js",
			{
				"name": "data-en.json",
				"callback": "DiscussionToolsHooks::getLocalData",
				"callbackParam": "en"
			},
			"parser.test.js"
		],
		"dependencies": [
			"ext.discussionTools.parser"
		]
	},
	"AutoloadClasses": {
		"DiscussionToolsHooks": "includes/DiscussionToolsHooks.php"
	},
	"Hooks": {
		"BeforePageDisplay": [
			"DiscussionToolsHooks::onBeforePageDisplay"
		]
	},
	"type": "other"
}
